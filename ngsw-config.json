{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "https://api.carlosduron.online/api/**",
        "https://87bp7xx8-3000.usw3.devtunnels.ms/api/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "5s"
      }
    },
    {
      "name": "api-images",
      "urls": [
        "https://api.carlosduron.online/**/*.jpg",
        "https://api.carlosduron.online/**/*.jpeg",
        "https://api.carlosduron.online/**/*.png",
        "https://api.carlosduron.online/**/*.webp",
        "https://87bp7xx8-3000.usw3.devtunnels.ms/**/*.jpg",
        "https://87bp7xx8-3000.usw3.devtunnels.ms/**/*.jpeg",
        "https://87bp7xx8-3000.usw3.devtunnels.ms/**/*.png",
        "https://87bp7xx8-3000.usw3.devtunnels.ms/**/*.webp"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "30d",
        "timeout": "5s"
      }
    }
  ]
}